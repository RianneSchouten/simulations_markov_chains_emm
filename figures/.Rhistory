orders <- plot_data %>%
filter(type == 'order') %>%
filter(measure != 'phiwrl') %>%
filter(measure != 'omegatv') %>%
filter(measure == 'phiaic' | measure == 'phiwd') %>%
mutate(measure = recode(measure, phiaic = "phiaic, phibic, phiaicc")) %>%
